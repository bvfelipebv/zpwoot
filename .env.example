# ============================================
# Configurações do Servidor
# ============================================
PORT=8080
ENVIRONMENT=development

# ============================================
# Configurações do Banco de Dados PostgreSQL
# ============================================
POSTGRES_DB=zpwoot
POSTGRES_USER=zpwoot
POSTGRES_PASSWORD=zpwoot_password_change_in_production
DATABASE_URL=postgres://zpwoot:zpwoot_password_change_in_production@localhost:5432/zpwoot?sslmode=disable

# ============================================
# Configurações de Segurança
# ============================================
API_KEY=your-secret-api-key-here

# ============================================
# Configurações de Log
# ============================================
LOG_LEVEL=info


